<!DOCTYPE html>
<html lang="en">
  <!-- head -->
  <%- include('partials/head'); -%>
  <!-- body -->
  <body class="text-center d-flex">
    <div class="d-flex w-100 p-3 mx-auto flex-column">
      <!-- header -->
      <%- include('partials/header'); -%>
      <div class="container">
        <div class="row">
          <!-- main -->
          <main class="main">
            <article>
              <h1 class="cover-heading mb-5"><%= model.title %></h1>
              <!-- pageMeta -->
              <%- postMeta %>
              <!-- mainContent-->
              <%- mainContent %>
              <!-- comments -->
              <%- comments %>
            </article>
          </main>
          <aside class="sidebar">
            <% if (model.useMediaNet) { %>
            <div id="740373428">
              <script type="text/javascript">
                try {
                  window._mNHandle.queue.push(function () {
                    window._mNDetails.loadTag(
                      "740373428",
                      "300x250",
                      "740373428"
                    );
                  });
                } catch (error) {}
              </script>
            </div>
            <div id="253719207">
              <script type="text/javascript">
                try {
                  window._mNHandle.queue.push(function () {
                    window._mNDetails.loadTag(
                      "253719207",
                      "300x250",
                      "253719207"
                    );
                  });
                } catch (error) {}
              </script>
            </div>
            <div id="564003572">
              <script type="text/javascript">
                try {
                  window._mNHandle.queue.push(function () {
                    window._mNDetails.loadTag(
                      "564003572",
                      "300x250",
                      "564003572"
                    );
                  });
                } catch (error) {}
              </script>
            </div>

            <% } else { %>
            <!-- ad unit-->
            <%- include('partials/ad-unit'); -%>
            <!-- ad unit-->
            <%- include('partials/ad-unit'); -%>
            <!-- ad unit-->
            <%- include('partials/ad-unit'); -%> <% } %>
            <!-- subscribe -->
            <%- include('partials/subscribe-form'); -%>
          </aside>
        </div>
      </div>
      <!-- footer -->
      <%- include('partials/footer'); -%>
    </div>
    <!-- scripts -->
    <%- include('partials/scripts'); -%>
  </body>
</html>
