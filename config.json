{
  "siteName": "James Edwards",
  "siteDescription": "James Edwards Freelance Web Developer Burlington Vermont",
  "version": "2.0.4",
  "saveContentGuid": { "enabled": true },
  "typescript": {
    "enabled": true,
    "ignoreGlobs": ["functions"]
  },
  "sass": { "enabled": true },
  "folders": {
    "content": { "rootFiles": { "copyToOutput": true } },
    "images": { "path": "images", "copyToOutput": true },
    "scripts": { "path": "scripts", "cacheBust": true }
  },
  "comments": { "enabled": true },
  "environment": {
    "dev": {
      "domain": "localhost:3000",
      "functionsUrl": "http://localhost:7071/api"
    },
    "staging": {
      "domain": ""
    },
    "production": {
      "domain": "https://www.jamesedwards.net",
      "functionsUrl": "https://jedwards.azurewebsites.net/api",
      "minifyHtml": true,
      "minifyTypescriptOutput": true,
      "sitemap": true,
      "rssFeed": true,
      "jsonApi": true,
      "inlineSassOutput": true
    }
  }
}
