<head>
  <% if (config.environment.googleAnalytics.enabled) { %>
  <script
    async
    src="https://www.googletagmanager.com/gtag/js?id=UA-141231909-1"
  ></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() {
      dataLayer.push(arguments);
    }
    gtag("js", new Date());
    gtag("config", "UA-141231909-1");
  </script>
  <% } %>
  <meta charset="utf-8" />
  <% if (model.metaDescription) { %>
  <meta name="description" content="<%= model.metaDescription %>" />
  <% } %>
  <meta
    name="viewport"
    content="width=device-width, initial-scale=1, shrink-to-fit=no"
  />
  <% if (config.environment.monetization.enabled && model.showAds) { %>
  <script
    async
    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"
  ></script>
  <% } %>
  <!--  -->
  <% if (config.environment.monetization.enabled) { %>
  <meta name="monetization" content="$ilp.uphold.com/UHNWGWxfXkNk" />
  <% } %>
  <!-- Bootstrap CSS -->
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
    integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2"
    crossorigin="anonymous"
  />
  <%- model.styles %>
  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
  <title><%= model.title %></title>
  <link rel="canonical" href="<%= model.canonical %>" />
  <link
    rel="alternate"
    type="application/rss+xml"
    title="RSS 2.0"
    href="<%=  config.environment.domain + '/feed.rss' %>"
  />
</head>
