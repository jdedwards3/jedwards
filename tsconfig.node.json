{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "built/",
    "sourceMap": true
  },
  "include": ["tools/**/*.ts"],
  "exclude": [
    "*.ts",
    "scripts/**/*.ts",
    "node_modules/**/*.ts",
    "fucntions/**/*.ts"
  ]
}
